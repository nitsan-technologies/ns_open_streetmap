<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<!--
			################################
			  SHEET General Settings
			################################
		-->
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ns_open_streetmap/Resources/Private/Language/locallang_db.xlf:tx_nsopenstreetmap_map.flexform.sheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.address>
						<TCEforms>
							<label>LLL:EXT:ns_open_streetmap/Resources/Private/Language/locallang_db.xlf:tx_nsopenstreetmap_map.flexform.address</label>
							<config>
								<type>select</type>
								<renderType>selectMultipleSideBySide</renderType>
								<foreign_table>tx_nsopenstreetmap_domain_model_address</foreign_table>
								<foreign_table_where>AND (tx_nsopenstreetmap_domain_model_address.sys_language_uid IN (-1,0))</foreign_table_where>
								<size>8</size>
								<maxitems>999</maxitems>
							</config>
						</TCEforms>
					</settings.address>
					<settings.height>
						<TCEforms>
							<label>LLL:EXT:ns_open_streetmap/Resources/Private/Language/locallang_db.xlf:tx_nsopenstreetmap_map.flexform.height</label>
							<config>
								<type>input</type>
								<default>500</default>
								<eval>required</eval>
								<range>
									<lower>500</lower>
									<upper>1000</upper>
								</range>
							</config>
						</TCEforms>
					</settings.height>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>